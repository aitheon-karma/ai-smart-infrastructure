{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ai-smart-infrastructure": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "ai",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/@aitheon/core-client/assets",
                "output": "/assets/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@aitheon/core-client/styles/themes",
                "output": "/styles/themes/"
              },
              {
                "glob": "**/*",
                "input": "./node_modules/@aitheon/app-viewer/src/assets",
                "output": "./assets/"
              }
            ],
            "styles": [
              "./node_modules/@aitheon/core-client/styles/main.css",
              "node_modules/@aitheon/app-viewer/src/styles/styles.scss",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/styles/main.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "5mb",
                  "maximumError": "6mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ai-smart-infrastructure:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ai-smart-infrastructure:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ai-smart-infrastructure:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/deeppurple-amber.css",
              "./node_modules/ngx-bootstrap/datepicker/bs-datepicker.css",
              "src/styles.scss"
            ],
            "scripts": [],
            "assets": [
              "src/assets",
              "node_modules/@aitheon/core-client/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ai-smart-infrastructure-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ai-smart-infrastructure:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ai-smart-infrastructure:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@aitheon/smart-infrastructure": {
      "root": "projects/aitheon/smart-infrastructure",
      "sourceRoot": "projects/aitheon/smart-infrastructure/src",
      "projectType": "library",
      "prefix": "ai",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/aitheon/smart-infrastructure/tsconfig.lib.json",
            "project": "projects/aitheon/smart-infrastructure/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/aitheon/smart-infrastructure/src/test.ts",
            "tsConfig": "projects/aitheon/smart-infrastructure/tsconfig.spec.json",
            "karmaConfig": "projects/aitheon/smart-infrastructure/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/aitheon/smart-infrastructure/tsconfig.lib.json",
              "projects/aitheon/smart-infrastructure/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ai-smart-infrastructure",
  "cli": {
    "analytics": false
  }
}
